{
  "name": "SquadBot",
  "version": "0.0.0",
  "description": "discord app bot",
  "main": "dist/index.js",
  "repository": "git@github.com:davidyorr/SquadBot.git",
  "author": "David Orr <davidyorr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn run tsc",
    "start": "node dist/index.js",

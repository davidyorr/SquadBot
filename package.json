    "canvas": "2.6.1",
    "discord.js": "12.4.1",
    "ffmpeg-extract-audio": "1.0.2",
    "league-charts": "1.1.4"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.6.4",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "concurrently": "5.3.0",
    "eslint": "7.11.0",
    "jest": "26.6.1",
    "nodemon": "2.0.6",
    "prettier": "2.1.2",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5"
  }
}
